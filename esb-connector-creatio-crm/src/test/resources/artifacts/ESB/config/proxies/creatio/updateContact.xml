<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="updateContact"
       startOnLoad="true"
       statistics="disable"
       trace="disable"
       transports="http,https">
<target>
    <inSequence>
        <property expression="json-eval($.contactId)" name="contactId"/>
        <creatiocrm.contactUpdate>
            <id>{$ctx:contactId}</id>
        </creatiocrm.contactUpdate>
        <respond/>
    </inSequence>
    <outSequence/>
    <faultSequence/>
</target>
<description/>
</proxy>

