<?xml version="1.0" encoding="UTF-8"?>
<proxy xmlns="http://ws.apache.org/ns/synapse"
       name="updateAccount"
       startOnLoad="true"
       statistics="disable"
       trace="disable"
       transports="http,https">
    <target>
        <inSequence>
            <property expression="json-eval($.accountId)" name="accountId"/>
            <creatiocrm.accountUpdate>
                <id>{$ctx:accountId}</id>
            </creatiocrm.accountUpdate>
            <respond/>
        </inSequence>
        <outSequence/>
        <faultSequence/>
    </target>
    <description/>
</proxy>
